function log(){

    var message;
    var login;

    if( login == 'prepod'){
        message = 'Hi!'}
    else if (login == 'boss'){
        message = 'Hello!'}
    else if (login == 'student'){
        message =  'Yoo!'}
    else{
        message = ' ';
    }
}

