function sw_ca()
{
    var arg = prompt('Enter number:')

    switch(arg){
    case '1':
    alert('one');

    case '2':
    alert('two');
    break;

    case '5':
    alert('non');

    case null:
    alert('cancel');

    default:
    alert('unknown number')
    }

}
